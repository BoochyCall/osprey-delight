{
    "@context": "http://schema.org",
    "@type": "{{ if isset .Params.workType . -}}{{- end }}",
    "url": "{{ .URL }}",
    // "aggregateRating": {
    //   "@type": "AggregateRating",
    //   "reviewCount": "0"
    // },
    "copyrightHolder": {
      "@type": "Person",
      "name": "{{ .Params.author }}"
    },
    "about": "{{ .Params.title }}",
    "copyrightYear": "{{ .Params.date }}",
    "description": "{{ .Params.alt }}",
    "creator": "{{ .Params.author }}",
    {{ with .Params.publicationDate }}"datePublished": "",{{- end }}
    {{ with .Params.tags }}"keywords": "{{ . }}",{{- end }}
    {{ with .Params.image -}}"thumbnailUrl": "{{ .}}",{{- end }}
    {{ with .Params.license -}}"license": "{{ . }}",{{- end }}
    {{ with .Params.lang -}}"inLanguage": "{{ . }}",{{- end }}
    "name": "{{ .Params.title }}",
    "creativeWorkStatus": "Published",
    "genre": "Tech",
  }